class Solution:
    def sortPeople(self, names: List[str], heights: List[int]) -> List[str]:
        n = len(heights)
        for i in range(n):
            min_len = i 
            for j in range(i+1,n):
                if heights[min_len] < heights[j]:
                   min_len = j
            
            heights[i],heights[min_len] = heights[min_len],heights[i]
            names[i],names[min_len] = names[min_len],names[i]

        return names



